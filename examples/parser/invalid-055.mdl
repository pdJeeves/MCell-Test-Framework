/****************************************************************************
 * Parser error 055: Concentration specified on spherical shell.
 *
 *    Expect an error on the line:
 *      CONCENTRATION = 42
 *
 * Author: Jed Wing <jed@salk.edu>
 * Date:   2008-04-04
 ****************************************************************************/

TIME_STEP = 1e-6
rad = [0, PI/4, PI/2, 3*PI/4]
rs RELEASE_SITE
{
  SHAPE = SPHERICAL_SHELL
  CONCENTRATION = 42
}
