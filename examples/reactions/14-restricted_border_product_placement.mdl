   DEFINE_SURFACE_REGIONS
	{
	   r1{
		ELEMENT_LIST = [RIGHT, TOP] 
                      
                MOLECULE_NUMBER{
                  b_A' = 400
                }

                 SURFACE_CLASS = refl_b_A 
	     }
            r2{
		ELEMENT_LIST = [RIGHT, BOTTOM] 

                MOLECULE_NUMBER{
                  b_B' = 400
                }
                
                 SURFACE_CLASS = refl_b_B 
            }
           r3{
		ELEMENT_LIST = [RIGHT] 
           }


           r4{
		ELEMENT_LIST = [TOP, BOTTOM, LEFT, FRONT, BACK] 
           }
        }
        TRANSLATE = [2.0, 0, 0]
}

