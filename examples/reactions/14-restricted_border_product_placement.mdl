                MOLECULE_NUMBER{
                  b_B' = 200
                }
                SURFACE_CLASS = refl_b_AB
	     }
	   r2{
		ELEMENT_LIST = [TOP,BOTTOM,LEFT,FRONT,BACK] 
	     }
	}
        TRANSLATE = [1.5, 0, 0]
}

