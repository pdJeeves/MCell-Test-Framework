    {
        [15, 15, 15]        // 000
        [15, 17, 15]        // 001
        [15, 17, 15]        // 010
        [15, 17, 17]        // 011
        [17, 15, 15]        // 100
        [17, 17, 15]        // 101
        [17, 17, 15]        // 110
        [17, 17, 17]        // 111
    }
    TET_ELEMENT_CONNECTIONS
    {
        [0, 1, 2, 4]
        [7, 6, 5, 3]
    }
    TRANSLATE = [-1, -1, -1]
    SCALE = [0.5, 0.5, 0.5]
}

